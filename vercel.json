{
    "name": "liquor-store-analytics",
    "version": 2,
    "routes": [
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "src": "/alerts",
            "dest": "/alerts.html"
        },
        {
            "src": "/report",
            "dest": "/report.html"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}